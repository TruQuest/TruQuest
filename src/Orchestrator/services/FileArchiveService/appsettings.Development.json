{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "OpenTelemetry": {
            "IncludeFormattedMessage": true,
            "IncludeScopes": true,
            "ParseStateValues": true
        }
    },
    "Otlp": {
        "Endpoint": "http://otel-collector:4317"
    },
    "Seq": {
        "Endpoint": "http://seq:5341/ingest/otlp/v1/logs"
    },
    "IPFS": {
        "Host": "http://ipfs:5001"
    },
    "Files": {
        "FetchBufferSizeBytes": 1024,
        "Images": {
            "AcceptableMimeTypes": "image/png,image/jpeg,image/webp",
            "UserImageMaxSizeKb": 1024
        },
        "TakeWebPageScreenshotsUsing": "Playwright"
    },
    "ApiFlash": {
        "Url": "https://api.apiflash.com/v1/urltoimage",
        "ScreenshotMaxSizeKb": 16384
    },
    "Playwright": {
        "WorkerCount": 4,
        "TargetAvgLoadPerWorker": 3
    },
    "Kafka": {
        "Brokers": [
            "kafka-1:9092"
        ],
        "Consumer": {
            "Topic": "requests",
            "GroupId": "FileArchiveService"
        },
        "Producer": {
            "Topic": "responses"
        }
    }
}
